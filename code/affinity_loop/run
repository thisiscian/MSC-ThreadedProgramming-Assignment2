#/bin/bash
#$ -pe omp 16 -N tp_loops2 -cwd -V -e output/error/$JOB_NAME-$JOB_ID -o output/$JOB_NAME-$JOB_ID 
input=src/loops2.c
program=bin/loops2
num_threads=2
if [ $HOSTNAME == 'netbook' ]; then
	./compile $input $program
	export OMP_NUM_THREADS=$num_threads
	./$program
else

	if [[ x$1x == xCOMx ]]; then
		#for num_threads in 2; do
		for num_threads in 16 12 8 6 4 2 1; do
			./compile $input $program
			export OMP_NUM_THREADS=$num_threads
			echo "num_threads = $num_threads"
			./$program
			echo ""
		done 
	else
		qsub $0 COM
	fi
fi
